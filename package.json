      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "js,html,css,json",
      "quiet": false
    }
  }
